{
  "name": "moh-fpincome",
  "version": "2.1.20",
  "description": "FPCare Income Review Web App",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bcgov/MOH-FPC-Enrolment.git"
  },
  "homepage": "git+https://github.com/bcgov/MOH-FPC-Enrolment.git/fpincome/README.md",
  "main": "src/main.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "test:coverage": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "test-headless-firefox": "ng test --watch=false --code-coverage --browsers=FirefoxHeadless",
    "dev": "ng serve --host=0.0.0.0 -o",
    "prebuild": "node src/version.js",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --versions.chrome=88.0.4324.150",
    "e2e": "ng e2e --webdriver-update=false",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "node src/version.js && npm run changelog",
    "postversion": "git add . && git commit -m \"chore: commit changelog\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.16",
    "@angular/common": "7.2.16",
    "@angular/compiler": "7.2.16",
    "@angular/core": "7.2.16",
    "@angular/forms": "7.2.16",
    "@angular/platform-browser": "7.2.16",
    "@angular/platform-browser-dynamic": "7.2.16",
    "@angular/router": "7.2.16",
    "@ng-select/ng-select": "^2.20.5",
    "angular2-text-mask": "^9.0.0",
    "angular2-uuid": "^1.1.1",
    "blueimp-canvas-to-blob": "~3.14.0",
    "blueimp-load-image": "~2.20.1",
    "bootstrap": "^4.5.3",
    "date-fns": "^2.16.1",
    "font-awesome": "^4.7.0",
    "moh-common-lib": "3.4.8",
    "moment": "^2.29.1",
    "mygovbc-bootstrap-theme": "^0.4.1",
    "ngx-bootstrap": "~5.1.2",
    "ngx-mydatepicker": "2.4.5",
    "pdfjs-dist": "2.11.338",
    "rxjs": "~6.4.0",
    "sha1": "^1.1.1",
    "text-mask-addons": "^3.8.0",
    "tslib": "^1.14.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.10",
    "@angular/cli": "7.3.10",
    "@angular/compiler-cli": "7.2.16",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "core-js": "2.5.7",
    "husky": "^4.3.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "^10.5.1",
    "prettier": "2.0.5",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "3.2.4",
    "zxcvbn": "4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,html,scss,md}": "prettier --write"
  }
}
